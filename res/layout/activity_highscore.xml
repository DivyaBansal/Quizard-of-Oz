<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background2"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Highscore" >

        <TextView
        android:id="@+id/h2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/h1"
        android:layout_alignRight="@+id/h1"
        android:textColor="#ffff00"
        android:layout_below="@+id/h1"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/s3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/h3"
        android:layout_alignLeft="@+id/s2"
        android:textColor="#ffff00"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/s2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/s1"
        android:layout_alignTop="@+id/h2"
        android:textColor="#ffff00"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/s2"
        android:layout_alignLeft="@+id/textView8"
        android:textColor="#ffff00"
        android:text="2"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/h3"
        android:layout_alignBottom="@+id/h3"
        android:layout_alignLeft="@+id/TextView01"
        android:textColor="#ffff00"
        android:text="3"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/h3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/h2"
        android:layout_below="@+id/s2"
        android:layout_marginTop="14dp"
        android:textColor="#ffff00"
        android:text="TextView"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/h1"
        android:layout_alignBottom="@+id/h1"
        android:layout_marginRight="17dp"
        android:layout_toLeftOf="@+id/h1"
        android:text="1"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffff00"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/s1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/h1"
        android:layout_marginRight="20dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#ffff00"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/h1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="45dp"
        android:text="TextView"
        android:textColor="#ffff00"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginRight="50dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hihh"
        android:textColor="#ffff00"
        android:textSize="40sp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_above="@+id/h1"
        android:layout_marginTop="24dp"
        android:layout_toLeftOf="@+id/textView1"
        android:src="@drawable/trophy" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_marginTop="24dp"
        android:layout_toRightOf="@+id/textView1"
        android:src="@drawable/trophy" />

</RelativeLayout>
